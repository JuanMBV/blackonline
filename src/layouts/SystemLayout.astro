---
import HeaderAcc from "../components/HeaderAcc.astro"

interface Props {
    pageTitle? : string
    pageDescription? : string
}

const {pageTitle, pageDescription} = Astro.props
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content={pageDescription}>
    <title>Blackonline | {pageTitle}</title>
</head>
<body>
    <HeaderAcc />
    <slot />
</body>
</html>